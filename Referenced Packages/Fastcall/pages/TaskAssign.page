<apex:page title="Assign call page has been moved" >

    <apex:slds />

    <div class="slds-scope">
        <div class="slds-grid slds-wrap slds-size_1-of-1 slds-align_absolute-center slds-p-around_xx-large">
            <apex:image url="{!URLFOR($Resource.FastCall__C2C, 'imgs/illustration-page-not-found.png')}"/>
            <div class="slds-size_1-of-1 slds-text-align_center">
